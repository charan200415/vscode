{
  "name": "code-server-heroku",
  "description": "A code-server environment designed to run in your Heroku",
  "repository": "https://github.com/charan200415/vscode",
  "logo": "https://avatars.githubusercontent.com/u/1375?s=200&v=4",
  "keywords": [
    "node",
    "express",
    "code-server",
    "cloud-development",
    "heroku-api"
  ],
    "env": {
      "PASSWORD": {
      "description": "Code-Server login password",
        "value" : "samplepass",
      "required" : true
      },
      "NO_SLEEP":{
      "description": "0 to Sleep , 1 to No Sleep",
            "value" : "1",
            "required" : true
      },
      "APP_NAME":{
      "description": "Your App Name Here",
            "required" : true
      },
      "RCLONE_DATA":{
      "description": "Your Rclone Data \n cat $(rclone config file | sed -n 2p) | base64 --wrap=0",
            "required" : true
      },
      "COMPILE_TIMEOUT":{
      "description": "COMPILE TIMEOUT",
            "value" : "3000",
            "required" : true
      }
        } 
  ,
  "stack": "container"
}
